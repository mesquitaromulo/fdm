Algoritmo "03-09-funcoesMatematicas"

Var

   a, x, y, z : real


Inicio

   x <- 25.0
   y <- 2.0
   z <- -25.0
   
   a <-RaizQ(x)
   escreval(a)
   
   a <- Exp(x, y)
   escreval(a)
   
   a <-Pi
   escreval(a:5:2)

   a <- Abs(z)
   escreval(z)
   escreval(a)



Fimalgoritmo